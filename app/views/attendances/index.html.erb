<div class="container-fluid">
  <div class="row">
    <div class="col">
      <center>
        <%= render partial: 'hello' %>
      </center>

      <h3><small>Your Class Attendance</small></h3>
      <table class="table table-bordered">
        <thead>
          <tr class="table-inverse text-center">
            <td>Day 📅</td>
            <td>Class Time 💻</td>
            <td>Time In 🕘 <small> (microseconds)</small></td>
            <td>On Time? ⏰</td>
            <td>Points 👾</td>
            <td>IP Address 😉</td>
            <td>Browser 😉</td>
          </tr>
        </thead>
        <tbody>
          <%= render partial: @attendances, cache: true %>
        </tbody>
      </table>
    </div>
  </div>
</div>




